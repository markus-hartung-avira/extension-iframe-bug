{
  "manifest_version": 2,
  "author": "Markus Hartung",
  "name": "Extension Iframe Bug",
  "description": "Demonstrates an iframe extension bug",
  "version": "1.0.0",
  "content_scripts": [{
    "matches": [
      "*://*/*"
    ],
    "js": [
      "content.js"
    ]
  }],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "iframe.html"
  ]
}
